{
  "name": "ohys-schedule",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start:parse": "yarn workspace @ohys/parse start",
    "prisma:generate": "yarn workspace @ohys/prisma-client run prisma:generate",
    "prisma:push": "yarn workspace @ohys/prisma-client run prisma:push",
    "prisma:studio": "yarn workspace @ohys/prisma-client run prisma:studio"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.196",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.24.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "packageManager": "yarn@3.1.1"
}
